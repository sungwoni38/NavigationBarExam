<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.more.MoreFragment">


    <Button
        android:id="@+id/btn_noti"
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="@color/logoNavy"
        android:fontFamily="@font/nanum_b"
        android:text="공지사항"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/h_gline_5_3" />

    <Button
        android:id="@+id/btn_qna"
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/logoNavy"
        android:fontFamily="@font/nanum_b"
        android:text="자주묻는질문"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_noti" />

    <Button
        android:id="@+id/btn_call"
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/logoNavy"
        android:fontFamily="@font/nanum_b"
        android:text="지역별 담당부서 전화번호"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_qna" />

    <Button
        android:id="@+id/btn_setting"
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/logoNavy"
        android:fontFamily="@font/nanum_b"
        android:text="설정"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_call" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanum_b"
        android:text="버전정보   1.0"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.127"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_setting" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gline_5_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.10943913" />


</androidx.constraintlayout.widget.ConstraintLayout>